{
    "name": "root/crud_admin",
    "description": "crud-admin for book and author",
    "authors": [
        {
            "name": "Shahrom",
            "email": "shahrommusaev@gmail.com"
        }
    ],
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "vendor/doctrine/doctrine-migrations-bundle": "^3.1",
        "vendor/easycorp/easyadmin-bundle": "^3",
        "vendor/symfony/console": "4.4.*",
        "vendor/symfony/dotenv": "4.4.*",
        "vendor/symfony/framework-bundle": "4.4.*",
        "vendor/symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "vendor/symfony/maker-bundle": "^1.33"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
